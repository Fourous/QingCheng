/* tabNav */

.list_tab {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 80rpx;
  /* background: white; */
  font-size: 16px;
  white-space: nowrap;
  position: block;
  border-bottom: 2rpx solid #ccc; 
  overflow: hidden;
}
.foot_item{
  /* background-color: #C9CACA; */
  height: 100%;
}
.list_tab_item {
   width: 21%;
   font-size: 30rpx;
   padding: 0 25rpx;
   display: inline-block;
   /* background-color: #C9CACA; */
   /* font-weight: bold; */
   
}

.list_tab_item.active {
   /* position: relative; */
   /* border-bottom: 3rpx solid #FF0000;  */
   color:#F7931E;
}

 .list_tab_item.active:after {
   content: "";
   display: block;
   height: 5rpx;
   width: 65%;
   background: #F7931E;
   position: absolute;
   bottom: 0;
   border-radius: 16rpx;
} 

.list_content {
   margin: 0 5rpx;
   border-top: 5rpx solid #C9CACA;
   background-color: #C9CACA;
}

.list_item {
   /* padding: 5rpx 5rpx; */
   width: 100%;
   float: left;
   border: 5rpx solid #C9CACA;
   background-color: white;
   display: flex;
}
.item_pic image {
   width: 100%;
