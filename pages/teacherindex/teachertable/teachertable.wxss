page{
  width: 100%;
  height: 100%;
  background-color: #E4E4E4;
}
.coachInfo{
  display: flex;
  background-color: #F6CA71;
  height: 20%;
  width: 100%;
  padding-left: 5%;
}
.coachText{
  height: 50%;
  margin-top: 53rpx;
  margin-left: 15px;
  font-size: 17px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.avater{
  margin-top: 50rpx;
  height: 130rpx;
  width: 130rpx;
  border-radius: 60rpx;
}
.warming{
  margin-top: 30px;
  font-size: 20px;
  display: flex;
  flex-direction: column;
  width: 95%;
  margin-left: 2.5%;
  color: black;
}
.timeTable {
  margin-top: 30px;
  margin-left: 2.5%;
  background-color: #F6CA71;
  color: #FFFFFF;
  vertical-align: top;
  text-align: center;
  width: 95%;
  height: 40%;
  display: inline-block;
}
.timeList {
  display: flex;
  flex-direction: row; 
  justify-content: space-around;
  text-align: center;
  background-color: #0099FF;
  height: 20%;
  font-size: 20px;
  font-weight: bold;
  width: 100%;
  
}
.timeItem {
  display: flex;
  flex-direction: column; 
  justify-content: center;
  text-align: center;
  width: 50%;
  border: 1rpx #eee solid;
}
